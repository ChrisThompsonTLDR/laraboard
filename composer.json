{
    "name": "christhompsontldr/laraboard",
    "description": "Laravel powered forums.",
    "authors": [
        {
            "name": "Chris Thompson",
            "email": "christhompsontldr@gmail.com",
            "homepage": "https://christhompsontldr.com"
        }
    ],
    "keywords": ["laravel","forum"],
    "support": {
        "issues": "https://github.com/christhompsontldr/laraboard/issues",
        "source": "https://github.com/christhompsontldr/laraboard"
    },
    "minimum-stability": "dev",
    "require": {
        "baum/baum": "^2.0",
        "artisanry/parsedown": "^2.2",
        "laravel/scout": "^8.0",
        "laravelcollective/html": "^6.0",
        "venturecraft/revisionable": "1.*",
        "santigarcor/laratrust": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Christhompsontldr\\Laraboard\\": "src/",
            "Christhompsontldr\\Laraboard\\Database\\Seeds\\": "database/seeds/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Christhompsontldr\\Laraboard\\LaraboardServiceProvider"
            ]
        }
    }
}
